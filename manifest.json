{
  "name": "Brave Wayback Machine Extension",
  "description": "__MSG_appDescription__",
  "version": "0.0.3",
  "manifest_version": 2,
  "default_locale": "en_US",
  "browser_action": {},
  "background": {
    "scripts": [
      "js/background.bundle.js",
      "js/utils.bundle.js"
    ],
    "persistent": true
  },
  "web_accessible_resources": [
    "img/404-image.svg"
  ],
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "*://*/*"
  ]
}
