{
  "compilerOptions": {
    "sourceMap": true,
    "module": "esnext",
    "target": "es2017",
    "jsx": "react",
    "moduleResolution": "node",
    "baseUrl": ".",
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": true
  },
  "exclude": [
      "node_modules",
  ]
}
